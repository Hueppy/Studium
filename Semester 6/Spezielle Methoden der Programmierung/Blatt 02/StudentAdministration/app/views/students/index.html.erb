<h1>Studenten</h1>
<table>
  <tr>
    <th>Matrikelnummer</th>
    <th>Vorname</th>
    <th>Nachname</th>
    <th>Aktionen</th>
  </tr>
  <% @students.each do |student| %>
  <tr>
    <td><%= student.matrikel %></td>
    <td><%= student.first_name %></td>
    <td><%= student.last_name %></td>
    <td><%= button_to "Delete", student_path(student), :method => "delete" %></td>
  </tr>
  <% end %>
</table>
